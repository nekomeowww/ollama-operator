{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "automerge": true,
    "extends": [
      "config:recommended",
      ":dependencyDashboard",
      ":semanticPrefixFixDepsChoreOthers",
      ":prHourlyLimitNone",
      ":prConcurrentLimitNone",
      ":ignoreModulesAndTests",
